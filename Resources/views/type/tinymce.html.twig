{% block tinymce_widget %}
    {{ block('form_widget') }}
    <script src="{{ asset('bundles/easycrud/tinymce/js/tinymce/tinymce.min.js') }}" referrerpolicy="origin"></script>
    <script>
        tinymce.init(
            {
                selector: "{{ "#" ~ form.vars.id }}"
            }
        );
    </script>
{% endblock %}